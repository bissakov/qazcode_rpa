{
  "env": {
    "CLAUDE_CODE_ENABLE_TELEMETRY": "0",
    "CLAUDE_BASH_MAINTAIN_PROJECT_WORKING_DIR": "true",
    "DISABLE_PROMPT_CACHING": "1"
  },
  "permissions": {
    "allow": [
      "Bash(cargo init:*)",
      "Bash(cargo build:*)",
      "Bash(cargo run:*)",
      "Bash(cargo fix:*)",
      "Bash(cargo clippy:*)",
      "Bash(cargo check:*)",
      "Bash(cargo test:*)",
      "Bash(bd onboard:*)",
      "Bash(bd create:*)",
      "Bash(bd update:*)"
    ]
  },
  "outputStyle": "default",
  "spinnerTipsEnabled": false
}
